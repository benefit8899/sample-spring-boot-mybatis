<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.example.web.home.dao.AuthorMapper">

	<select id="getAllAuthor"
		resultType="com.example.web.home.entity.AuthorEntity">
		SELECT *
		FROM AUTHOR
		WHERE 1=1
	</select>
</mapper>
